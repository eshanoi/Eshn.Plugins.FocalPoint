<?xml version="1.0" encoding="utf-8"?>
<module>
	<dojo>
		<!-- Add a mapping from Mosey to ~/ClientResources/Scripts to the dojo loader configuration -->
		<paths>
			<add name="focal-point" path="Scripts" />
		</paths>
	</dojo>
	<clientResources>
		<add name="editor.js" path="ClientResources/focal-point/Scripts/editor.js" resourceType="Script"/>
	</clientResources>
	<clientModule initializer="focal-point/editor">
		<moduleDependencies>
			<add dependency="CMS" type="RunAfter" />
		</moduleDependencies>
	</clientModule>
</module>