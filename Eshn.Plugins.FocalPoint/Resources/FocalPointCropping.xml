<?xml version="1.0" encoding="utf-8" ?>
<languages>
	<language name="English" id="en">
		<contenttypes>
			<imagedata>
				<properties>
					<focalpoint>
						<caption>Focal point</caption>
						<help>The point in the image, where the focus should be, automatically cropped images will be calculated based on this point.</help>
					</focalpoint>
					<originalheight>
						<caption>Height</caption>
						<help>The image height in pixels.</help>
					</originalheight>
					<originalwidth>
						<caption>Width</caption>
						<help>The image width in pixels.</help>
					</originalwidth>
				</properties>
			</imagedata>
		</contenttypes>
		<focalpoint>
			<clearbutton>Remove focal point</clearbutton>
		</focalpoint>
	</language>
	<language name="svenska" id="sv">
		<contenttypes>
			<imagedata>
				<properties>
